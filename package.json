{
  "name": "rememberry",
  "vesrion": "0.1.0",
  "description": "Flashcard app",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "20"
  },
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "backend": "pnpm --filter backend",
    "frontend": "pnpm --filter frontend",
    "format": "biome format . --write",
    "format-check": "biome format .",
    "docker-db-init": "./scripts/start_docker_environment",
    "docker-db-reset": "docker compose down postgres_database && bun run docker-db-init",
    "docker-backend-build": "docker compose up -d --build rembackend ",
    "docker-backend-start": "docker compose start rembackend",
    "docker-backend-stop": "docker compose stop rembackend",
    "docker-backend-delete": "docker compose down rembackend",
    "docker-delete-all": "docker compose down"
  },
  "dependencies": {
    "@tanstack/react-query": "v4.18",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "superjson": "^2.2.1",
    "typescript": "^5.4.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.3",
    "@tanstack/eslint-plugin-query": "^4.38.0",
    "@types/node": "20.5.9",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.6"
  }
}
